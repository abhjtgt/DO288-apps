pipeline {
  options {
   timeout(time: 10, unit: 'MINUTES')
  }
  agent {
    node {
     label 'master' 
    }
  }
  
  environment {
   PRO = "jentry2"
   APP = "myjen"
  }
  
  stages {
    stage('Build') {
      steps {
       echo "** Building" 
       echo "Project: ${PRO}"
      }
    }
    
    stage('Test') {
      steps {
       echo "** Testing" 
      }
    }
    
    stage('Deploy') {
      steps {
       echo "** Deploying" 
      }
    }
  }
}
