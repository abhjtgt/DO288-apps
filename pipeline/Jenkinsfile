pipeline {
  options {
    timeout( time:30, unit:'MINUTES') 
  }
  environment {
   NAME = "ABHIJIT" 
    APPNAME = "pipeline"
    NEXUS_SERVER = "http://nexus-common.apps.na45.prod.nextcle.com/repository/java"
  }
  agent {
    node {
      label 'maven' 
    }
  }
  
  stages {
    stage("BUILD") {
      steps {
        sh '''
          cd $APPNAME
          echo "Compiling on $NEXUS_SERVER"
          mvn -s settings.xml clean compile
          
          echo "Packaging"
          mvn -s settings.xml clean package
        '''
      }
    }
  }
}
